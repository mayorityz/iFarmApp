<?xml version="1.0"?>  
<configuration>  
    <system.webServer>  
        <rewrite>  
            <rules>  
                <rule name="react routes" stopProcessing="true">
	                <match url="^"  />
	                <action type="Rewrite" url="/index.html"  appendQueryString="true" />
                </rule>
            </rules>  
        </rewrite>  
    </system.webServer>  
</configuration>   